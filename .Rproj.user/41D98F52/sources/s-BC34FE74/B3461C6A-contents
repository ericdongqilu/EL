####
Request.Step3.else <-function(...) {
  Dots <- list(...)
  print(names(Dots))
  
  ChosenEpscode <- Dots$Epscode
  
  #  
  Url = paste0(Environment.practice,Ep.info.ep.ready)
  
  print(c(ChosenEpscode,Url))
  
  Url
}

