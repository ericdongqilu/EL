#Source
source("packages/package.R")



####
Request.Step3.6<-function(...) {
  Dots <- list(...)
  print(names(Dots))
  
  ChosenEpscode <- Dots$Epscode
  
  #  
  Url <- paste0(Environment.practice,
                Ep.info.ep.ready,
                Ep.Query(CEPScode = ChosenEpscode))
  Url
}

#Request.Step3.6(Epscode=ChosenEpscode)

####
