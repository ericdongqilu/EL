Request.Step6.PUT <-function(...) {
  Dots <- list(...)
  print(names(Dots))

  Url <- Dots$rUrl
  Headers <- Dots$rHeaders
  Body <- Dots$rBody
  #
  r <- PUT(url = Url,
           Headers,
           body = Body)
  r
}



#' A Cat Function
#'
#' This function allows you to express your love of cats.
#' @param ... Do you love cats? Defaults to TRUE.
#' @keywords cats
#' @export
#' @examples
#'
#'
#'
#'
#

Request.Step6.GET <-function(...) {
  Dots <- list(...)
  print(names(Dots))

  Url <- Dots$rUrl
  Headers <- Dots$rHeaders
  Body <- Dots$rBody
  #
  r <- GET(url = Url,
           Headers)
  r
}
