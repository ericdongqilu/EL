#Source
source("packages/package.R")

#
Request.Step6.PATCH <-function(...) {
  Dots <- list(...)
  print(names(Dots))
  
  Url <- Dots$rUrl
  Headers <- Dots$rHeaders
  Body <- Dots$rBody
  #  
  r <- PATCH(url = Url,
             Headers,
             body = Body)
  r
}
#
Request.Step6.POST <-function(...) {
  Dots <- list(...)
  print(names(Dots))
  
  Url <- Dots$rUrl
  Headers <- Dots$rHeaders
  Body <- Dots$rBody
  #  
  r <- POST(url = Url,
            Headers,
            body = Body)
  r
}
#
Request.Step6.PUT <-function(...) {
  Dots <- list(...)
  print(names(Dots))
  
  Url <- Dots$rUrl
  Headers <- Dots$rHeaders
  Body <- Dots$rBody
  #  
  r <- PUT(url = Url,
           Headers,
           body = Body)
  r
}
#
Request.Step6.GET <-function(...) {
  Dots <- list(...)
  print(names(Dots))
  
  Url <- Dots$rUrl
  Headers <- Dots$rHeaders
  Body <- Dots$rBody
  #  
  r <- GET(url = Url,
           Headers)
  r
}



