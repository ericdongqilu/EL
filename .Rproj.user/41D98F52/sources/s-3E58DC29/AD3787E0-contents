#Source
source("packages/package.R")
source("requestep/header/token/TokenInfo.R")

# Ep.Header Function
Ep.Header <- function(pToken = chosenTOKEN){
  
  EpHeader <- add_headers(c("Content-Type "="application/json",
                            "Authorization " = paste0("Bearer ",pToken)
                            ))
  return(EpHeader)
}


